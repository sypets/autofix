{
	"name": "sypets/test-extension",
	"type": "typo3-cms-extension",
	"description": "Only for testing",
	"license": [
		"GPL-2.0-or-later"
	],
	"keywords": [
		"TYPO3",
		"TYPO3 extension"
	],
	"authors": [
		{
			"name": "Sybille Peters",
			"email": "sypets@gmx.de",
			"role": "Developer"
		}
	],
	"config": {
		"sort-packages": true,
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	},
	"require": {
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "test_extensions",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	},
	"autoload": {
		"psr-4": {
		}
	}
}
